    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://www.illinois.edu/advise/Firewall">
  <owl:Ontology rdf:about="http://www.illinois.edu/advise/Firewall">
