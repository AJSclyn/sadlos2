  </owl:Ontology>
  <owl:Class rdf:ID="ApplicationLayerFirewall">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Firewall"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="FirewallHosted">
    <rdfs:subClassOf rdf:resource="http://www.illinois.edu/advise/System#Host"/>
  </owl:Class>
  <owl:Class rdf:ID="FirewallRule">
    <rdfs:label>firewall rule</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="PermittingFirewall">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.illinois.edu/advise/Firewall#Firewall"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="FirewallAppliance">
    <rdfs:subClassOf rdf:resource="http://www.illinois.edu/advise/System#Device"/>
  </owl:Class>
  <owl:Class rdf:ID="Connection"/>
