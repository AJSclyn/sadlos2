  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="blockedConnectionTo">
    <rdfs:domain rdf:resource="http://www.illinois.edu/advise/System#System"/>
    <rdfs:range rdf:resource="http://www.illinois.edu/advise/System#System"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="implicitlyPermittedConnectionTo">
    <rdfs:subPropertyOf>
