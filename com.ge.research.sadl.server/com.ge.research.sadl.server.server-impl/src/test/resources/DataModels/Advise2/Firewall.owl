        </owl:unionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="BlockingFirewall">
    <rdfs:subClassOf rdf:resource="http://www.illinois.edu/advise/Firewall#Firewall"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="firewallRule">
    <rdfs:domain rdf:resource="http://www.illinois.edu/advise/Firewall#Firewall"/>
