    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="connectionTo">
    <rdfs:domain rdf:resource="http://www.illinois.edu/advise/Firewall#Connection"/>
    <rdfs:range rdf:resource="http://www.illinois.edu/advise/System#System"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="blocksConnection">
