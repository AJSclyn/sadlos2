      <owl:Class rdf:about="http://www.illinois.edu/advise/Firewall#Firewall"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://www.illinois.edu/advise/Firewall#Firewall">
