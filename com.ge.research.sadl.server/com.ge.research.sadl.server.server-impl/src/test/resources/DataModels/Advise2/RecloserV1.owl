<rdf:RDF
    xmlns:srvc="http://www.illinois.edu/advise/Service#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns="http://www.illinois.edu/advise/RecloserV1#"
    xmlns:sys="http://www.illinois.edu/advise/System#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:firewall="http://www.illinois.edu/advise/Firewall#"
    xmlns:adversary="http://www.illinois.edu/advise/Adversary#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://www.illinois.edu/advise/RecloserV1">
  <owl:Ontology rdf:about="http://www.illinois.edu/advise/RecloserV1">
    <owl:imports rdf:resource="http://www.illinois.edu/advise/ActivitySequenceGeneration"/>
    <owl:imports rdf:resource="http://www.illinois.edu/advise/Firewall"/>
    <owl:imports rdf:resource="http://www.illinois.edu/advise/Attack"/>
    <owl:imports rdf:resource="http://com.ge.research/testgesadl/Service"/>
    <owl:imports rdf:resource="http://www.illinois.edu/advise/System"/>
    <owl:versionInfo>$Revision:$ Last modified on   $Date:$</owl:versionInfo>
    <rdfs:comment xml:lang="en">This ontology was created from a SADL file 'RecloserV1.sadl' and should not be edited.</rdfs:comment>
  </owl:Ontology>
  <owl:Class rdf:ID="LineCond">
    <rdfs:subClassOf rdf:resource="http://www.illinois.edu/advise/System#Condition"/>
  </owl:Class>
  <owl:Class rdf:ID="RecloserCond">
    <rdfs:subClassOf rdf:resource="http://www.illinois.edu/advise/System#Condition"/>
  </owl:Class>
  <owl:Class rdf:ID="GoodHost">
    <rdfs:subClassOf rdf:resource="http://www.illinois.edu/advise/System#Host"/>
  </owl:Class>
  <owl:Class rdf:ID="Recloser">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.illinois.edu/advise/RecloserV1#RecloserCond"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasRecloserCond"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.illinois.edu/advise/System#Device"/>
  </owl:Class>
  <owl:Class rdf:ID="Line">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.illinois.edu/advise/RecloserV1#LineCond"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasLineCond"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.illinois.edu/advise/System#Device"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="http://www.illinois.edu/advise/RecloserV1#hasLineCond">
    <rdfs:domain rdf:resource="http://www.illinois.edu/advise/RecloserV1#Line"/>
    <rdfs:range rdf:resource="http://www.illinois.edu/advise/RecloserV1#LineCond"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.illinois.edu/advise/RecloserV1#hasRecloserCond">
    <rdfs:domain rdf:resource="http://www.illinois.edu/advise/RecloserV1#Recloser"/>
    <rdfs:range rdf:resource="http://www.illinois.edu/advise/RecloserV1#RecloserCond"/>
  </owl:ObjectProperty>
  <Recloser rdf:ID="SysWideRecloser">
    <hasRecloserCond>
      <RecloserCond rdf:ID="Closed"/>
    </hasRecloserCond>
  </Recloser>
  <RecloserCond rdf:ID="Open"/>
  <srvc:Electricity rdf:ID="LocalElectricSvc">
    <srvc:requires>
      <srvc:Service rdf:ID="SSCtrlService">
        <srvc:requires>
          <sys:Controller rdf:ID="SubstationEquip"/>
        </srvc:requires>
      </srvc:Service>
    </srvc:requires>
  </srvc:Electricity>
  <adversary:HackerGroup rdf:ID="Anonymous"/>
  <Line rdf:ID="Line1">
    <hasLineCond>
      <LineCond rdf:ID="Good"/>
    </hasLineCond>
  </Line>
  <Recloser rdf:ID="LocalRecloser">
    <hasRecloserCond rdf:resource="http://www.illinois.edu/advise/RecloserV1#Closed"/>
  </Recloser>
  <firewall:Firewall rdf:ID="InternetCorporateLANFW">
    <firewall:firewallRule>
      <firewall:FirewallRule>
        <firewall:permitsConnection>
          <firewall:Connection>
            <firewall:connectionTo>
              <sys:Network rdf:ID="CorporateLAN">
                <sys:connectedTo>
                  <sys:Network rdf:ID="Internet"/>
                </sys:connectedTo>
              </sys:Network>
            </firewall:connectionTo>
            <firewall:connectionFrom>
              <GoodHost/>
            </firewall:connectionFrom>
          </firewall:Connection>
        </firewall:permitsConnection>
      </firewall:FirewallRule>
    </firewall:firewallRule>
  </firewall:Firewall>
  <LineCond rdf:ID="Faulty"/>
  <sys:DataAtRest rdf:ID="SSGroupSettings">
    <sys:managedby>
      <sys:Application rdf:ID="HMIApp">
        <sys:function>
          <sys:Control>
            <sys:target>
              <sys:Gateway rdf:ID="SubstationGWGroup">
                <sys:connectedTo>
                  <sys:Network rdf:ID="RecloserRadioNet"/>
                </sys:connectedTo>
                <sys:connectedTo>
                  <sys:Network rdf:ID="SCADACommNW"/>
                </sys:connectedTo>
              </sys:Gateway>
            </sys:target>
          </sys:Control>
        </sys:function>
      </sys:Application>
    </sys:managedby>
    <sys:managedby>
      <sys:Application rdf:ID="EngApp">
        <sys:function>
          <sys:Control>
            <sys:target>
              <sys:Gateway rdf:ID="SubstationGW">
                <sys:connectedTo rdf:resource="http://www.illinois.edu/advise/RecloserV1#RecloserRadioNet"/>
                <sys:connectedTo rdf:resource="http://www.illinois.edu/advise/RecloserV1#SCADACommNW"/>
              </sys:Gateway>
            </sys:target>
          </sys:Control>
        </sys:function>
      </sys:Application>
    </sys:managedby>
    <sys:storageDevice>
      <sys:Controller rdf:ID="SubstationEquipGroup"/>
    </sys:storageDevice>
  </sys:DataAtRest>
  <sys:Workstation rdf:ID="SubStationHMI">
    <sys:connectedTo rdf:resource="http://www.illinois.edu/advise/RecloserV1#SCADACommNW"/>
  </sys:Workstation>
  <sys:Workstation rdf:ID="EngWkstn">
    <sys:connectedTo>
      <sys:Network rdf:ID="EngRemAccessNW">
        <sys:connectedTo rdf:resource="http://www.illinois.edu/advise/RecloserV1#SCADACommNW"/>
        <sys:connectedTo rdf:resource="http://www.illinois.edu/advise/RecloserV1#Internet"/>
      </sys:Network>
    </sys:connectedTo>
  </sys:Workstation>
  <srvc:Electricity rdf:ID="WideElectricSvc">
    <srvc:requires>
      <srvc:Service rdf:ID="SSCtrlServiceGroup">
        <srvc:requires rdf:resource="http://www.illinois.edu/advise/RecloserV1#SubstationEquipGroup"/>
      </srvc:Service>
    </srvc:requires>
  </srvc:Electricity>
  <firewall:Firewall rdf:ID="CorpLANSCADALANFW">
    <firewall:firewallRule>
      <firewall:FirewallRule>
        <firewall:permitsConnection>
          <firewall:Connection>
            <firewall:connectionTo>
              <sys:Network rdf:ID="SCADALAN">
                <sys:connectedTo rdf:resource="http://www.illinois.edu/advise/RecloserV1#SCADACommNW"/>
                <sys:connectedTo rdf:resource="http://www.illinois.edu/advise/RecloserV1#CorporateLAN"/>
              </sys:Network>
            </firewall:connectionTo>
            <firewall:connectionFrom rdf:resource="http://www.illinois.edu/advise/RecloserV1#CorporateLAN"/>
          </firewall:Connection>
        </firewall:permitsConnection>
      </firewall:FirewallRule>
    </firewall:firewallRule>
  </firewall:Firewall>
  <sys:OperatingSystem rdf:ID="Windows7">
    <sys:subsystemof>
      <GoodHost rdf:ID="Laptop1">
        <sys:connectedTo rdf:resource="http://www.illinois.edu/advise/RecloserV1#Internet"/>
      </GoodHost>
    </sys:subsystemof>
  </sys:OperatingSystem>
  <sys:DataAtRest rdf:ID="SSsettings">
    <sys:managedby rdf:resource="http://www.illinois.edu/advise/RecloserV1#HMIApp"/>
    <sys:managedby rdf:resource="http://www.illinois.edu/advise/RecloserV1#EngApp"/>
    <sys:storageDevice rdf:resource="http://www.illinois.edu/advise/RecloserV1#SubstationEquip"/>
  </sys:DataAtRest>
  <firewall:Firewall rdf:ID="InternetEngrWorkstationFW">
    <firewall:firewallRule>
      <firewall:FirewallRule>
        <firewall:permitsConnection>
          <firewall:Connection>
            <firewall:connectionTo rdf:resource="http://www.illinois.edu/advise/RecloserV1#EngRemAccessNW"/>
            <firewall:connectionFrom>
              <GoodHost/>
            </firewall:connectionFrom>
          </firewall:Connection>
        </firewall:permitsConnection>
      </firewall:FirewallRule>
    </firewall:firewallRule>
  </firewall:Firewall>
  <adversary:ForeignGovernment rdf:ID="China"/>
  <sys:OperatingSystem rdf:ID="Linux">
    <sys:subsystemof>
      <sys:Host rdf:ID="HackerPC">
        <sys:connectedTo rdf:resource="http://www.illinois.edu/advise/RecloserV1#Internet"/>
      </sys:Host>
    </sys:subsystemof>
  </sys:OperatingSystem>
</rdf:RDF>
